@model IEnumerable<MyCalendar.Model.Calendar>
@{
    ViewBag.Title = "My Calendar";
}

<h1>My Calendar</h1>

<table>
    <thead>
        <tr>
            <th>#</th>
            <th>Item</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Any())
        {

            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="2">No Calendar found</td>
            </tr>
        }
    </tbody>
</table>